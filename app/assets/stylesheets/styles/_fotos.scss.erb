
figure{
	margin: 0;
}

figcaption {
	display: none;
}

.fotos-th {
  @include thumb(
    // Width of border around thumbnail
    $border-width: 0px,
    // Box shadow to apply to thumbnail
    $box-shadow: 0 0 0 0.5px rgba(#000,.2),
    // Box shadow to apply on hover
    $box-shadow-hover: 0 0 6px 1px rgba(#ccc,0.5)
  );

}

.fit_w{
	width: 100%;
}
.pic1{
	&>li:nth-child(2){
    	padding-bottom: 0px;
	}
}
@media only screen and (min-width: 64.063em) { 
	.pic1{
		&>li:first-child{
			width: 66.66666666%;
		}
		&>li:nth-child(2){
			width: 33.33333333%;
		}
	}
}